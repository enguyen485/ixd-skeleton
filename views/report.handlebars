<html>
<head>
	<title>View Report</title>
	<link href="css/bootstrap.css" rel="stylesheet">
	<link href="css/bootstrap-theme.min.css" rel="stylesheet">
	<link href="css/main.css" rel="stylesheet">
	<!-- icons stylesheet -->
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
</head>
<body style="font-family:'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif">
<div id="navbar">
	<a href="favorite"><i id="btn" class="fa fa-heart fa-lg"></i></a>
	<a href="search"><i id="btn" class="fa fa-search fa-lg"></i></a>
	<a href="profile"><i id="btn" class="fa fa-user fa-lg"></i></a>
</div>
<div class="container">


		<div class="Information">
			<h1>Current Reported Cases</h1>
			<hr>
			<h3>Your Saved Locations:</h3>
			<table>
				<tr>
					<th>Location</th>
					<th>Positive Cases</th>
					<th>Total Reported</th>
				</tr>
			{{#each SavedLocations}}
				<div class ="Locations">
					<tr>
						<th>{{Place}}</th>
						<th>{{Positive}}</th>
						<th>{{Total}}</th>
					</tr>
				</div>
			{{/each}}
			</table>
		</div>
</div>
	<script src="https://code.jquery.com/jquery.js"></script>
	<script src="/js/introHCI.js"></script>
</body>
</html>
